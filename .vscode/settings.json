// Platzieren Sie Ihre Einstellungen in dieser Datei, um Standard- und Benutzereinstellungen zu Ã¼berschreiben.
{
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/tmp": true,
        "**/debian/*": true,
        "**/doc/*": true,
        "**/.pybuild/*": true,
        "**/coverage/*": true,
        "**/dist/*": true,
        "**/coverage": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.egg-info": true
    },
    "pgsql.connection": "postgres://username:password@host:port/database",
    "python.linting.pylintPath": "pylint",
    "vsicons.presets.angular": true,
    "git.ignoreLimitWarning": true,
    "javascript.implicitProjectConfig.checkJs": true,
    "ngComponents.componentGlobs": [
        "**/**/**/*.component.ts",
        "**/**/**/*.component.js"
    ]
}